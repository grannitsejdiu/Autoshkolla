<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_alert_window_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="co.tenton.admin.autoshkolla.BlurryBackgrounds_Activities.AlertWindow_Activity">


    <TextView
        android:id="@+id/txtpyetje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:text="A dëshironi të anuloni këtë test?"
        />

    <Button
        android:id="@+id/btnVazhdo"
        android:layout_below="@id/txtpyetje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:textColor="#FFFFFF"
        android:text="VAZHDO"
        android:background="@drawable/roundedbuttonwhite"/>

    <Button
        android:id="@+id/btnAnulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnVazhdo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:textColor="#FFFFFF"
        android:text="ANULO"
        android:background="@drawable/roundedbuttonred"/>

</RelativeLayout>
